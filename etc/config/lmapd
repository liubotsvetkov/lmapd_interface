
config agent 'agent'
	option agent_id '587d9c7e-c65e-4d9c-a9c6-9aac1774287b'
	option group_id '42'
	option report_group_id '1'
	option report_agent_id '0'

config schedule 'report_primary'
	option name 'report_primary'
	option start 'every_minute'
	option execution_mode 'parallel'
	list action 'action_report'

config capabilities 'capabilities'
	list task_names 'lmap_reporter'
	list task_names 'ping'
	list task_names 'traceroute'

config task 'lmap_reporter'
	option name 'lmap_reporter'
	option program '/bin/lmap_reporter'
	option version '0.1'

config action 'action_report'
	option name 'action_report'
	option task 'lmap_reporter'
	option execution_mode 'sequential'
	option report_action '1'

config event 'every_minute'
	option name 'every_minute'
	option event_type '0'
	option interval '60'

config task 'ping'
	option name 'ping'
	option program '/bin/ping'
	option version '0.6'

config task 'traceroute'
	option name 'traceroute'
	option program '/bin/traceroute'
	option version '0.6'

config basic_tasks 'basic_tasks'
	list task_names 'basictask1'
	list task_names 'basictask2'
	list task_names 'basictask3'
	list task_names 'basictask4'
	list task_names 'basictask5'
	list task_names 'basictask6'
	list task_names 'basictask7'
	list task_names 'basictask8'
	list task_names 'basictask9'
	list task_names 'basictask5'

config task_option 'basic_option_basictask1_ping_count'
	option id 'basic_option_basictask1_ping_count'
	option name '-c'
	option value '4'

config task_option 'basic_option_basictask1_ping_size'
	option id 'basic_option_basictask1_ping_size'
	option name '-s'
	option value '32'

config action 'basic_action_basictask1'
	option name 'basic_action_basictask1'
	option task 'ping'
	list destination 'report_primary'
	option host_name '8.8.8.8'
	list task_options 'basic_option_basictask1_ping_count'
	list task_options 'basic_option_basictask1_ping_size'

config event 'basic_event_basictask1'
	option name 'basic_event_basictask1'
	option event_type '0'
	option interval '300'

config schedule 'basic_schedule_basictask1'
	option name 'basic_schedule_basictask1'
	option start 'basic_event_basictask1'
	option execution_mode 'sequential'
	list action 'basic_action_basictask1'

config task_option 'basic_option_basictask2_ping_count'
	option id 'basic_option_basictask2_ping_count'
	option name '-c'
	option value '4'

config task_option 'basic_option_basictask2_ping_size'
	option id 'basic_option_basictask2_ping_size'
	option name '-s'
	option value '32'

config action 'basic_action_basictask2'
	option name 'basic_action_basictask2'
	option task 'ping'
	list destination 'report_primary'
	option host_name '8.8.8.8'
	list task_options 'basic_option_basictask2_ping_count'
	list task_options 'basic_option_basictask2_ping_size'

config event 'basic_event_basictask2'
	option name 'basic_event_basictask2'
	option event_type '1'
	list month '*'
	list day_of_month '*'
	list day_of_week '*'
	list hour '*'
	list minute '*'
	list second '0'

config schedule 'basic_schedule_basictask2'
	option name 'basic_schedule_basictask2'
	option start 'basic_event_basictask2'
	option execution_mode 'sequential'
	list action 'basic_action_basictask2'

